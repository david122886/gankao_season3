window.items = "<%= @items_str %>";
window.ids = "<%= @ids_str %>";
<% if @redirct == "true" && !@items_str.empty? %>
  $.ajax({
  async:true,
  dataType:'script',
  url:"/learn/task_dispatch?category=<%= cookies[:category] %>",
  type:'post',
  data:{
  items : window.items,
  ids : window.ids
  }
  })
<% elsif @items_str.empty? && @status == false %>
  show_tip("#pass");
<% elsif @items_str.empty? && @status == true %>
  show_tip("#all_pass");
<% elsif @flag == "true" %>
  show_tip("#right");
<% elsif @flag == "false" %>
  show_tip("#wrong");
<% end %>