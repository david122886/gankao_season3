window.items = "<%= @items_str %>";
window.ids = "<%= @ids_str %>";
<% if @redirct == "true" && !@items_str.empty? %>
  $.ajax({
  async:true,
  dataType:'script',
  url:"/learn/task_dispatch?category=<%= cookies[:category] %>",
  type:'post',
  data:{
  items : window.items,
  ids : window.ids
  }
  })
<% elsif @items_str.empty? && @status == false %>
  window.clearInterval(local_timer);
  $("#all_pass .ts_text").html("太棒了，你已经完成了任务" + $(".pl_l_bg").attr("id") + "，要坚持每天二十道真题才能保证学习质量哦，继续努力哦。");
  show_tip("#all_pass");
<% else%>
  show_tip("#write_tip");
<% end %>