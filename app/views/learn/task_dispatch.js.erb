<% if @result
  type = @result[:type].to_i
  step = @result[:step].to_i
  case type
  when UserPlan::CHAPTER_TYPE_NUM[:WORD]
    if step != 3
    %>
      $(".plan_tab").html("<%= escape_javascript(render :partial => "/learn/word_12") %>");
    <% else %>
      $(".plan_tab").html("<%= escape_javascript(render :partial => "/learn/word_3") %>");
    <%
    end
  when UserPlan::CHAPTER_TYPE_NUM[:SENTENCE]
    if step == 1
    %>
      $(".plan_tab").html("<%= escape_javascript(render :partial => "/learn/sentence_read") %>");
    <% else %>
      $(".plan_tab").html("<%= escape_javascript(render :partial => "/learn/sentence_combin") %>");
    <%
    end
  when UserPlan::CHAPTER_TYPE_NUM[:LISTEN]
  %>
    alert("listen");
  <% end %>
<% else %>
  alert("finish");
<% end %>
myTime.start(<%= @result[:time] %>);