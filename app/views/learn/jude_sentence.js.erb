window.items = "<%= @items_str %>";
window.ids = "<%= @ids_str %>";
<% if @redirct == "true" && !@items_str.empty? %>
  $.ajax({
  async:true,
  dataType:'script',
  url:"/learn/task_dispatch?category=<%= cookies[:category] %>",
  type:'post',
  data:{
  items : window.items,
  ids : window.ids
  }
  })
<% elsif @items_str.empty? && @status == false %>
  window.clearInterval(local_timer);
  $("#pass #text1").html("恭喜你完成了句子部分的练习。");
  $("#pass #text2").html("下面听力训练，继续努力。");
  show_tip("#pass");
<% elsif @items_str.empty? && @status == true %>
  window.clearInterval(local_timer);
  $("#all_pass .ts_text").html("太棒了，你已经完成了任务" + $(".pl_l_bg").attr("id") + "，离成功又近了一步，继续努力哦。");
  show_tip("#all_pass");
<% elsif @flag == "true" %>
  show_tip("#right");
<% elsif @flag == "false" %>
  show_tip("#wrong");
<% end %>